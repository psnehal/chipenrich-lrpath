Package: chipenrich
Type: Package
Title: Gene set enrichment for ChIP-seq peak data
Version: 1.7.2
Date: 2015-05-16
Authors@R: c(
  person(c("Ryan","P."),"Welch",role = c("aut","cre","cph"),email = "welchr@umich.edu"),
  person("Chee","Lee",role = c("aut","cre"),email = "cheelee@umich.edu"),
